<div class="panel panel-default">
  <div class="panel-body">
    <div class = "row">
      <h3 class = "col-md-6">Cryptos</h3>
      <div class = "form-inline col-md-6">
        <%= form_tag home_show_path, :method => 'POST' do %>
          <div class = "form-group">
            <%= text_field_tag 'sym', nil, placeholder: 'Search Crypto by Symbol', size: 50, class: "form-control"  %>
          </div>
          <div class="form-group">
            <%= submit_tag 'Search', class: "btn btn-primary"%>
          </div>
                
        <% end %>
      </div>
    </div>
  </div>
